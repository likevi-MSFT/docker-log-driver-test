{
	"description": "jsonfilelog as plugin",
	"documentation": "-",
	"entrypoint": ["/sblogdriver"],
	"interface": {
		"types": ["docker.logdriver/1.0"],
		"socket": "jsonfile.sock"
	},
	"mounts": [
		{
			"source": "/mnt/docker/logdriver/logs",
			"destination": "/mnt/docker/logdriver/logs",
			"type": "bind",
			"options": ["rbind", "rw"]
		}
	],
	"env": [
		{
			"name": "LOG_LEVEL",
			"description": "Set log level to output for plugin logs",
			"value": "info",
			"settable": ["value"]
		}
	]
}


